<h3>All Companies</h3>

  <% @companies.each do |company| %>
  <ul>
    <li><%= link_to company.name, company_path(company) %></li>
    <li><%= link_to "Edit", edit_company_path(company) %></li>
    <li><%= link_to "Delete", company_path(company), method: "delete" %></li>
  </ul>
  <% end %>
<p><%= link_to "New Company", new_company_path %></p>
