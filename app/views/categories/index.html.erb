<h1>All Categories</h1>

<% @categories.each do |category|  %>
  <p><%= category.title %></p>
  <p><%= link_to "Delete", category_path(category), method: :delete %></p>
  <p><%= link_to "Edit", edit_category_path(category) %></p>
<% end %>
<p><%= link_to "Create new category", new_category_path %></p>
